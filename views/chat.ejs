<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>whykaycraft portfolio</title>
    <script src="https://kit.fontawesome.com/c7394a92be.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">        
    <link rel= "stylesheet" type="text/css" href="/stylesheet/app.css">
    <link rel="shortcut icon" href="/img/whykaycraft.ico" />
    <link rel="apple-touch-icon" href="/img/whykaycraft.ico" />
</head>
<body>

        <div class="booking-list">
            <table class="table">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Name</th>
                        <th>E-mail</th>
                        <th>Message</th>
                    </tr>
                </thead>
                <tbody>
                    <% messages.forEach(function(message) { %>
                    <tr>
                        <td><%= message.date %></td>
                        <td><%= message.name %></td>
                        <td><a href="mailto:<%= message.email %>?subject=whykaycraft%20Portfolio"><%= message.email %></a></td>
                        <td><%= message.message%></td>
                        
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>

    </body>
    </html>